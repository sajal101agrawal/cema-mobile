{
  "expo": {
    "name": "CEMA",
    "slug": "CEMA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/CemaSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ECF3FA"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icon/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ecommerce.cema",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/Images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "915cc8f6-6fec-4307-9019-64467b0d4b35"
      }
    }
  }
}
